    "sizeFrom": 2,//尺寸来自
    "sizeTo": 0,//将大小调整为
    "colorFrom": "FFFFF7FF",//这里为颜色,透明度是最后两位
    "colorTo": "7FFF0090",
    "cone":360 },//角度
]},
}
